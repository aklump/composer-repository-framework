{
  "name": "aklump/composer-repository-framework",
  "description": "Composer package manager for unpublished In the Loft Studios packages.",
  "type": "project",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Aaron Klump",
      "email": "sourcecode@intheloftstudios.com"
    }
  ],
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/aklump/annotated-response"
    },
    {
      "type": "github",
      "url": "https://github.com/aklump/gitignore"
    },
    {
      "type": "github",
      "url": "https://github.com/aklump/easy-perms"
    }
  ],
  "autoload": {
    "psr-4": {
      "AKlump\\Packages\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AKlump\\ComposerRepositoryFramework\\": "src",
      "AKlump\\ComposerRepositoryFramework\\Tests\\Unit\\": "./tests_unit"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "ext-json": "*",
    "composer/satis": "^2.0",
    "aklump/annotated-response": "^0.0",
    "vlucas/phpdotenv": "^5.6",
    "monolog/monolog": "^2.10",
    "ext-curl": "*",
    "aklump/easy-perms": "^0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  }
}
